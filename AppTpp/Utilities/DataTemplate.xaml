<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:AppTpp.View"
    xmlns:vm="clr-namespace:AppTpp.ViewModel">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:InputDataBatchVM}">
        <view:InputDataBatch />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:KelolaDataVM}">
        <view:KelolaData />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LihatLaporanVM}">
        <view:LihatLaporan />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FormatAripVM}">
        <view:FormatArip />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FormatSimgajiVM}">
        <view:FormatSimgaji />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BendaharaVM}">
        <view:BendaharaPengeluaran />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UserManagerVM}">
        <view:UserManager />
    </DataTemplate>

</ResourceDictionary>